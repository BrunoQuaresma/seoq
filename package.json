{
  "name": "seoq",
  "version": "0.1.0",
  "type": "module",
  "description": "A CLI tool for SEO analysis, comparison, and optimization",
  "main": "dist/index.js",
  "bin": {
    "seoq": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node --watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "seo",
    "analysis",
    "comparison",
    "optimization",
    "cli",
    "tool",
    "command-line",
    "seoq"
  ],
  "author": {
    "name": "Bruno Quaresma",
    "url": "https://github.com/BrunoQuaresma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrunoQuaresma/seoq.git"
  },
  "bugs": {
    "url": "https://github.com/BrunoQuaresma/seoq/issues"
  },
  "homepage": "https://github.com/BrunoQuaresma/seoq#readme",
  "license": "MIT",
  "dependencies": {
    "@langchain/openai": "^1.2.1",
    "@playwright/browser-chromium": "^1.57.0",
    "chalk": "^5.6.2",
    "cheerio": "^1.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "fast-xml-parser": "^4.3.5",
    "langchain": "^1.2.6",
    "ora": "^9.0.0",
    "p-limit": "^7.2.0",
    "playwright": "^1.48.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.52.0"
  }
}
